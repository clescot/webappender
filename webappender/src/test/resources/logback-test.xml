<?xml version="1.0"?>
<configuration>

    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>- %msg%n</pattern>
        </encoder>
    </appender>
    <root level="all">
        <appender-ref ref="STDOUT"/>
    </root>
    <logger name="com.clescot" level="debug"/>


</configuration>